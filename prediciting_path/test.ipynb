    "import py3Dmol\n",
    "\n",
    "def visualize_protein(pdb_code):\n",
    "    view = py3Dmol.view(query='pdb:' + pdb_code)\n",
    "    view.setStyle({'cartoon': {'color': 'spectrum'}})\n",
    "    view.show()\n",
    "\n",
    "# Example usage\n",
    "visualize_protein('1BNA')"
   ]
